{
  "private": false,
  "name": "@open-oracle-origami/origami-js-sdk",
  "version": "0.0.0-development",
  "description": "Open Oracle Origami Node JS SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "package": "yarn build && yarn pack",
    "clean": "node tools/cleanup",
    "cm": "cz",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-oracle-origami/origami-js-sdk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Lion's Habitat",
    "email": "lion@creaselabs.io",
    "url": "https://github.com/lionshabitat"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "open",
    "oracle",
    "origami",
    "sdk",
    "curator",
    "operator",
    "web3",
    "decentralized",
    "blockchain"
  ],
  "bugs": {
    "url": "https://github.com/open-oracle-origami/origami-js-sdk/issues"
  },
  "homepage": "https://github.com/open-oracle-origami/origami-js-sdk#readme",
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.3",
    "@types/pubsub-js": "^1.8.3",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.2.1",
    "semantic-release": "^21.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@types/superagent": "*",
    "superagent": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "pubsub-js": "^1.9.4",
    "ws": "^8.13.0"
  }
}
